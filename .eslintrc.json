{
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery":true
    },
    "parser": "babel-eslint",
    "plugins": [
        "prettier",
    ],
    "parserOptions": {
        "version": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
